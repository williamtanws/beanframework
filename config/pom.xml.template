			<version>3.0.0</version>
		</dependency>
		<dependency>
			<groupId>com.sample</groupId>
			<artifactId>trainingbackoffice</artifactId>
			<version>3.0.0</version>
		</dependency>
		-->
	</dependencies>

</project>